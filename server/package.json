    "@types/jsonwebtoken": "^7.2.8",
    "@types/jwt-decode": "2.2.1",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "express": "4.16.4",
    "express-session": "1.15.6",
